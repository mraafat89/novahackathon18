<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BasicActivity"
    tools:layout_editor_absoluteY="25dp">

    <EditText
        android:id="@+id/editTextOrigin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="11909 Market St, Reston, VA, 20190"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="72dp" />

    <EditText
        android:id="@+id/editTextDestination"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/editTextOrigin"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="2231 Crystal Dr, Arlington, VA, 22202"
        tools:layout_editor_absoluteX="66dp"
        tools:layout_editor_absoluteY="162dp" />

    <Button
        android:id="@+id/buttonSearch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="51dp"
        android:text="Search"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="245dp" />

    <SeekBar
        android:id="@+id/seekBarCal"
        android:layout_width="239dp"
        android:layout_height="44dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/seekBarRisk"
        android:layout_marginBottom="174dp"
        android:max="10" />

    <SeekBar
        android:id="@+id/seekBarCost"
        android:layout_width="239dp"
        android:layout_height="44dp"
        android:layout_alignStart="@+id/seekBarRisk"
        android:layout_centerVertical="true"
        android:max="10" />

    <SeekBar
        android:id="@+id/seekBarTime"
        android:layout_width="239dp"
        android:layout_height="44dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/seekBarRisk"
        android:layout_marginTop="185dp"
        android:max="10" />

    <SeekBar
        android:max="10"
        android:id="@+id/seekBarRisk"
        android:layout_width="239dp"
        android:layout_height="44dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="14dp"
        android:layout_marginTop="117dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="190dp"
        android:layout_marginStart="12dp"
        android:text="Calories Burned" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="50dp"
        android:text="Cost" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="198dp"
        android:text="Duration" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_marginTop="130dp"
        android:text="Feeling Lucky" />
</RelativeLayout>